<script type="text/javascript">
function isUsername()
{
  var usernam=document.getElementById('uname').value;
  var usr=document.getElementById('usr');
  if(usernam!="")
  {
 	if(!usernam.match('^[0-9a-zA-Z._]+'))
	{
		usr.innerHTML="Username cannot have special characters!";
		return false;
	}
	
   	else if(usernam.length<5 || usernam.length>20)
   	{
		usr.innerHTML="Username must be between 5-20 characters!";
		return false;
   	}
   	
	else
   	{
		usr.innerHTML="&#10004";
		return true;
   	} 
  }
  else if(usernam.length==0)
   {
	usr.innerHTML="Please enter a Username!";
	return false;
   }
}

function chkpassword()
{
   var pawd1=document.getElementById('pass').value;
   var palt=document.getElementById('pwd');
 if(pawd1.length==0)
  {
    palt.innerHTML="Please enter the password!";
    return false;
  }
  else if(pawd1.length<6 || pawd1.length>12)
  {
	palt.innerHTML="Password must be between 6-12 characters!";
        return false;
  }
 else
  {
    palt.innerHTML="&#10004";
    return true;
  }
}

function confpassword()
{
  var pawd=document.getElementById('pass').value;
  var rpawd=document.getElementById('rpass').value;
  var pass=document.getElementById('rpwd');
  
  if (pawd!=rpawd)
  {
     pass.innerHTML="Passwords don't match!";
     return false;
  }
  else if(rpawd=="")
  {
     pass.innerHTML="Please re-enter the password!";
     return false;
  }
  else
  {
     pass.innerHTML="&#10004";
     return true;
  }

}

function chkform()
 {
	if(isUsername())
	{
		if(chkpassword())
		{
			if(confpassword())
                        {
 		 		return true;
                	}
 			else
			{
			return false;
			}
		}
		else
		{
			return false;
		}
	}
	else
	{
		return false;
	}
}
</script>
</head>
<body>
<div>
      <form class="form-signin" action="" method="post" align="center">
        <h2 class="form-signin-heading">To Do List</h2>
        <h3 class="form-signin-heading">Please SignUp</h3>
        <input type="text" class="input-block-level" name="uname" placeholder="Username" id="uname" onBlur="isUsername()"><span id="usr" class="error"></span>
        <input type="password" class="input-block-level" name="password" placeholder="Password" id="pass" onBlur="chkpassword()"><span id="pwd" class="error"></span>
        <input type="password" class="input-block-level" placeholder="Re-enter Password" id="rpass" onBlur="confpassword()"><span id="rpwd" class="error"></span>
        <button class="button" type="submit" onclick="return chkform()">Sign Up</button>
        <a href="/login" style="float:right;">Sign In</a>
      </form>
</div>
